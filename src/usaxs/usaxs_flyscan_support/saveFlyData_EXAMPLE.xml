                    <PV label="pi_flag" pvname="9idc:bss:proposal:user8:piFlag" string="true" />
                </group>
                <group name="user9" class="NXuser">
                    <attribute name="canSAS_class" value="SASuser" />
                    <PV label="last_name" pvname="9idc:bss:proposal:user9:lastName" string="true" />
                    <PV label="first_name" pvname="9idc:bss:proposal:user9:firstName" string="true" />
                    <PV label="user_id" pvname="9idc:bss:proposal:user9:userId" string="true" />
                    <PV label="badge_number" pvname="9idc:bss:proposal:user9:badgeNumber" string="true" />
                    <PV label="email" pvname="9idc:bss:proposal:user9:email" string="true" />
                    <PV label="institution" pvname="9idc:bss:proposal:user9:institution" string="true" />
                    <PV label="institution_id" pvname="9idc:bss:proposal:user9:instId" string="true" />
                    <PV label="pi_flag" pvname="9idc:bss:proposal:user9:piFlag" string="true" />
                </group>

                <group name="sample" class="NXsample">
                    <attribute name="canSAS_class" value="SASsample" />
                    <PV label="name" pvname="usxLAX:USAXS:sampleTitle" string="true" />
                    <PV label="thickness" pvname="usxLAX:USAXS:SampleThickness" />
                    <PV label="description" pvname="usxSample:Description" />
                    <PV label="chemical_formula" pvname="usxSample:ChemicalFormula" />

                    <PV label="temperature" pvname="usxSample:Temperature">
                        <attribute name="units" value="C" />
                    </PV>
                    <PV label="concentration" pvname="usxSample:Concentration">
                        <attribute name="units" value="g/cm3" />
                    </PV>
                    <PV label="volume_fraction" pvname="usxSample:VolumeFraction" />
                    <PV label="scattering_length_density" pvname="usxSample:ScatteringLengthDensity">
                        <attribute name="units" value="cm/cm3" />
                    </PV>
                    <PV label="magnetic_field" pvname="usxSample:MagneticField" />
                    <PV label="stress_field" pvname="usxSample:StressField" />
                    <PV label="electric_field" pvname="usxSample:ElectricField">
                        <attribute name="units" value="V" />
                    </PV>
                    <PV label="x_translation" pvname="usxSample:XTranslation">
                        <attribute name="units" value="mm" />
                    </PV>
                    <PV label="rotation_angle" pvname="usxSample:RotationAngle">
                        <attribute name="units" value="degrees" />
                    </PV>

                    <PV label="magnetic_field_direction" pvname="usxSample:MagneticFieldDir" />
                    <PV label="stress_field_direction" pvname="usxSample:StressFieldDir" />
                    <PV label="electric_field_direction" pvname="usxSample:ElectricFieldDir" />
                </group>

                <group name="flyScan" class="NXdata">
                    <!-- this raw data group is not SASdata, that comes from data reduction -->
                    <attribute name="signal"     value="mca3" />

                    <!-- positioners -->
                    <PV label="AR_PulsePositions" pvname="usxLAX:traj1:PulsePositions">
                        <attribute name="units" value="degrees" />
                    </PV>
                    <PV label="AR_NumPulsePositions" pvname="usxLAX:traj1:NumPulsePositions" />
                    <PV label="AR_PulseMode" pvname="usxLAX:traj1:PulseMode" />
                    <!--
                       PulseMode is a string describing the type of scan:

                       1. Fixed: fixed pulses as before (we should have that nailed down by now already)
                       2. Array: use PulsePositions
                       3. TrajPts: use trajectory points (the 200 points in current trajectory)
                    -->

                    <PV label="AR_waypoints" pvname="usxLAX:traj1:M1Traj" length_limit="AR_count">
                        <attribute name="units" value="degrees" />
                    </PV>
                    <PV label="AR_count" pvname="usxLAX:traj1:Nelements" />
                    <PV label="AR_pulses" pvname="usxLAX:traj1:Npulses">
                        <attribute name="units" value="pulses" />
                    </PV>
                    <PV label="AR_start" pvname="usxLAX:USAXS:ARstart">
                        <!-- pushed here by SPEC usaxs_flyscan.txt macro -->
                        <attribute name="units" value="degrees" />
                    </PV>
                    <PV label="AR_increment" pvname="usxLAX:USAXS:MinStep">
                        <attribute name="units" value="degrees" />
                    </PV>
                    <PV label="AR_MinStepTraj" pvname="usxLAX:USAXS:MinStepTraj">
                        <!-- minimum step of trajectory waypoints -->
                        <attribute name="units" value="degrees" />
                    </PV>
                    <PV label="AR_Finish" pvname="usxLAX:USAXS:Finish">
                        <attribute name="units" value="degrees" />
                    </PV>
                    <PV label="StartOffset" pvname="usxLAX:USAXS:StartOffset">
                        <attribute name="units" value="degrees" />
                    </PV>

                    <PV label="time_elapsed" pvname="usxLAX:traj1:ElapsedTime" acquire_after_scan="true">
                        <attribute name="units" value="s" />
                    </PV>
                    <PV label="time_total" pvname="usxLAX:traj1:Time">
                        <attribute name="units" value="s" />
                    </PV>


                    <!-- detectors -->
                    <PV label="mca_clock_frequency" pvname="usxLAX:3820:clock_frequency">
                        <attribute name="units" value="1/s" />
                    </PV>
                    <PV label="mca_channels" pvname="usxLAX:3820:CurrentChannel" acquire_after_scan="true" />
                    <PV label="mca_channel_max" pvname="usxLAX:3820:MaxChannels" />
                    <PV label="mca_channels_used" pvname="usxLAX:3820:NuseAll" acquire_after_scan="true">
                        <attribute name="meaning" value="number of Struck channels to use" />
                    </PV>

                    <PV label="mca1_name" pvname="usxLAX:3820:scaler1.NM1" />
                    <PV label="mca1" pvname="usxLAX:3820:mca1" length_limit="mca_channels"  acquire_after_scan="true">     <!-- counts of 50 MHz clock -->
                        <attribute name="units" value="pulses" />
                        <attribute name="USAXS_name" value="clock_pulses" />
                    </PV>

                    <PV label="mca2_name" pvname="usxLAX:3820:scaler1.NM2" />
                    <PV label="mca2" pvname="usxLAX:3820:mca2" length_limit="mca_channels" acquire_after_scan="true">
                        <attribute name="units" value="counts" />
                        <attribute name="USAXS_name" value="I0" />
                    </PV>

                    <PV label="mca3_name" pvname="usxLAX:3820:scaler1.NM3" />
                    <PV label="mca3" pvname="usxLAX:3820:mca3" length_limit="mca_channels" acquire_after_scan="true">
                        <attribute name="signal" value="1" />
                        <attribute name="units" value="pulses" />
                        <!-- <attribute name="axes" value="AR" /> -->
                        <!-- The proper ordinate is obtained via the MAC channel index:
                            AR[i] = AR_start + AR_increment * i

                            Can the AR array be made available in EPICS?
                        -->
                        <attribute name="USAXS_name" value="upd" />
                    </PV>

                    <!-- 2014-02-28, PRJ
                        The following PVs record amplifier gain changes
                        and the corresponding MCS channel number at which the change occurred
                        for each of four detector amplifiers.  (Two monitors and two UPD amplifiers)
                        Choice of the UPD amplifier in use is found in: usxRIO:femto:model
                        which takes either of these two values:
                        * 0: "DLPCA200"
                        * 1: "DDPCA300"
                        -->

                    <PV label="upd_flyScan_amplifier"         pvname="usxRIO:femto:model" />   <!--saves as number, not string-->
                    <PV label="upd_flyScan_amplifier_ZNAM"    pvname="usxRIO:femto:model.ZNAM" />
                    <PV label="upd_flyScan_amplifier_ONAM"    pvname="usxRIO:femto:model.ONAM" />

                                        <!-- these record when the amplifiers have changed gain during the fly scan -->
                    <PV label="changes_I0_mcsChan"       pvname="usxLAX:USAXSfly:I0:mcsChan" acquire_after_scan="true" />
                    <PV label="changes_I0_ampGain"       pvname="usxLAX:USAXSfly:I0:ampGain" acquire_after_scan="true" />
                    <PV label="changes_I0_ampReqGain"    pvname="usxLAX:USAXSfly:I0:ampReqGain" acquire_after_scan="true" />

                    <PV label="changes_I00_mcsChan"      pvname="usxLAX:USAXSfly:I00:mcsChan" acquire_after_scan="true" />
                    <PV label="changes_I00_ampGain"      pvname="usxLAX:USAXSfly:I00:ampGain" acquire_after_scan="true" />
                    <PV label="changes_I00_ampReqGain"   pvname="usxLAX:USAXSfly:I00:ampReqGain" acquire_after_scan="true" />

                    <PV label="changes_DLPCA200_mcsChan"    pvname="usxLAX:USAXSfly:DLPCA200:mcsChan" acquire_after_scan="true" />
                    <PV label="changes_DLPCA200_ampGain"    pvname="usxLAX:USAXSfly:DLPCA200:ampGain" acquire_after_scan="true" />
                    <PV label="changes_DLPCA200_ampReqGain" pvname="usxLAX:USAXSfly:DLPCA200:ampReqGain" acquire_after_scan="true" />

                    <PV label="changes_DDPCA300_mcsChan"    pvname="usxLAX:USAXSfly:DDPCA300:mcsChan" acquire_after_scan="true" />
                    <PV label="changes_DDPCA300_ampGain"    pvname="usxLAX:USAXSfly:DDPCA300:ampGain" acquire_after_scan="true" />
                    <PV label="changes_DDPCA300_ampReqGain" pvname="usxLAX:USAXSfly:DDPCA300:ampReqGain" acquire_after_scan="true" />

                    <!-- 2016-06-21,prj:  PV not needed and removed from IOC now, take it out here, too
                    <PV label="AYDYstart" pvname="usxLAX:USAXSfly:AYDYstart">
                        <attribute name="meaning"
                            value="point number when to start moving ay and dy stages" />
                    </PV>
                    -->
                    <PV label="dyMaxSpeed" pvname="usxLAX:USAXSfly:dyMaxSpeed" />
                    <PV label="ayMaxSpeed" pvname="usxLAX:USAXSfly:ayMaxSpeed" />
                    <PV label="arMaxSpeed" pvname="usxLAX:USAXSfly:arMaxSpeed" />
                    <!-- do not save these three PVs, they are not quite the same as the three above
                        <PV label="FS_AyMaxSpeed" pvname="usxLAX:USAXS:FS_AyMaxSpeed" />
                        <PV label="FS_DyMaxSpeed" pvname="usxLAX:USAXS:FS_DyMaxSpeed" />
                        <PV label="FS_ArMaxSpeed" pvname="usxLAX:USAXS:FS_ArMaxSpeed" />
                    -->
                    <PV label="FS_ElapsedTime" pvname="usxLAX:USAXS:FS_ElapsedTime" acquire_after_scan="true" />

                    <PV label="FS_NumberOfPoints" pvname="usxLAX:USAXS:FS_NumberOfPoints">
                        <attribute name="meaning" value="number of waypoints in trajectory scan" />
                    </PV>
                    <PV label="FS_ScanTime" pvname="usxLAX:USAXS:FS_ScanTime" />

                    <PV label="changes_AR_PSOpulse"    pvname="usxLAX:USAXSfly:AR:mcsChan" acquire_after_scan="true" />
                    <PV label="changes_AR_angle"      pvname="usxLAX:USAXSfly:AR:pos" acquire_after_scan="true" >
                        <attribute name="meaning"      value="AR readback when PSO pulse fired with 10Hz jitter" />
                    </PV>

                    <PV label="is_2D_USAXS_scan"    pvname="usxLAX:USAXS:is2DUSAXSscan" >
                        <attribute name="meaning"     value="does this scan use 2D collimated geometry" />
                    </PV>

                </group>    <!-- entry:NXentry/flyScan:NXdata -->


                <!-- metadata -->
                <group name="metadata" class="NXcollection">    <!-- metadata:NXcollection -->

                    <!-- beam line conditions -->
                    <PV label="SR_current" pvname="S:SRcurrentAI">
                        <attribute name="units" value="A" />
                    </PV>
                    <PV label="ID_E" pvname="ID09:Energy">
                        <attribute name="units" value="keV" />
                    </PV>
                    <PV label="DCM_energy" pvname="9ida:BraggERdbkAO">
                        <attribute name="units" value="keV" />
                    </PV>
                    <PV label="DCM_wavelength" pvname="9ida:BraggLambdaRdbkAO">
                        <attribute name="units" value="Angstrom" />
                    </PV>
                    <PV label="DCM_theta" pvname="9ida:m10.RBV">
                        <attribute name="units" value="degrees" />
                    </PV>

                    <!-- beam line mirrors -->
                    <PV label="VDM_Stripe" pvname="9ida:m5.RBV" />
                    <PV label="VDM_Pitch" pvname="9ida:m6.RBV" />
                    <PV label="VDM_Vertical" pvname="9ida:m7.RBV" />
                    <PV label="VDM_T" pvname="9ida:m8.RBV" />

                    <PV label="mir_slitb" pvname="9ida:m28.RBV" />
                    <PV label="mir_slitl" pvname="9ida:m27.RBV" />
                    <PV label="mir_slitr" pvname="9ida:m25.RBV" />
                    <PV label="mir_slitt" pvname="9ida:m26.RBV" />

                    <!-- SPEC -->
                    <PV label="SPEC_macro_file_string19" pvname="usxLAX:string19" string="true" />
                    <PV label="SPEC_macro_file" pvname="usxLAX:macroFile" string="true" />
                    <PV label="SPEC_data_file" pvname="usxLAX:specFile" string="true" />
                    <PV label="SPEC_data_dir" pvname="usxLAX:USAXS:userDir" string="true" />
                    <PV label="spec_scan" pvname="usxLAX:USAXS:specScan" string="true" />
                    <PV label="state" pvname="usxLAX:USAXS:state" string="true" />
                    <PV label="timeStamp" pvname="usxLAX:USAXS:timeStamp" string="true" />

                    <!-- USAXS terms -->
                    <PV label="analyzer_distance" pvname="usxLAX:USAXS:SAD" />
                    <PV label="detector_distance" pvname="usxLAX:USAXS:SDD" />
                    <PV label="UATerm" pvname="usxLAX:USAXS:UATerm" />
